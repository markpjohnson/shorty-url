<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<system.web>
		<httpRuntime enableVersionHeader="false" />
	</system.web>
	<system.webServer>
		<security>
			<requestFiltering removeServerHeader="true" />
		</security>
	    <httpProtocol>
	      <customHeaders>
	        <remove name="X-Powered-By"/>
	        <remove name="X-AspNet-Version"/>
	      </customHeaders>
	    </httpProtocol>
	</system.webServer>
</configuration>